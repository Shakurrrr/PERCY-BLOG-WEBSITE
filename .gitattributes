# Ignore generated/static assets in language stats
static/**                linguist-generated
assets/**                linguist-generated
**/*.min.js              linguist-generated
**/*.min.css             linguist-generated
**/*.map                 linguist-generated

# Treat tool configs as vendored (excluded)
tailwind.config.js       linguist-vendored
postcss.config.js        linguist-vendored
ui.preset.js             linguist-vendored
